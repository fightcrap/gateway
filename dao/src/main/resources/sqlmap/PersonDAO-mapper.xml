        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
        namespace="com.fightingcrap.server.dao.dao.PersonMapper">

    <select id="getById" parameterType="long" resultType="com.fightingcrap.server.dao.entity.Person">
